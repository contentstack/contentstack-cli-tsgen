    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@oclif/plugin-help": "^3.2.0",
    "@types/async": "^3.2.18",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.162",
    "@types/node": "^10.17.28",
    "async": "^3.2.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "jest": "^26.5.3",
    "oclif": "^3.7.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
