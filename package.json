{
  "name": "contentstack-cli-tsgen",
  "description": "Generate TypeScript typings from a Stack.",
  "version": "4.3.0",
  "author": "Michael Davis",
  "bugs": "https://github.com/Contentstack-Solutions/contentstack-cli-tsgen/issues",
  "dependencies": {
    "@contentstack/cli-command": "^1.5.0",
    "@contentstack/cli-utilities": "^1.11.0",
    "@contentstack/types-generator": "^3.4.0",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@oclif/plugin-help": "^6.2.27",
    "@oclif/test": "^4.1.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^8.57.0",
    "eslint-config-oclif": "^6.0.42",
    "eslint-config-oclif-typescript": "^3.1.14",
    "jest": "^29.7.0",
    "oclif": "^4.17.43",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Contentstack-Solutions/contentstack-cli-tsgen",
  "keywords": [
    "contentstack",
    "cli",
    "plugin",
    "typescript"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "csdx",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Contentstack-Solutions/contentstack-cli-tsgen",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc.js --fix",
    "prepack": "rm -rf lib && tsc -b && oclif manifest && oclif readme",
    "test": "jest --testPathPattern=tests",
    "version": "oclif readme && git add README.md",
    "test:integration": "jest --testPathPattern=tests/integration"
  },
  "csdxConfig": {
    "shortCommandName": {
      "tsgen": "TSGEN"
    }
  }
}
