  "version": "2.1.0",
  "author": "Michael Davis",
  "bugs": "https://github.com/Contentstack-Solutions/contentstack-cli-tsgen/issues",
  "dependencies": {
    "@contentstack/cli-command": "^1.2.0",
    "@contentstack/cli-utilities": "^1.2.0",
    "lodash": "^4.17.20",
