{
  "name": "contentstack-cli-tsgen",
  "description": "Generate TypeScript typings from a Stack.",
  "version": "3.5.0",
  "author": "Michael Davis",
  "bugs": "https://github.com/Contentstack-Solutions/contentstack-cli-tsgen/issues",
  "dependencies": {
    "@contentstack/cli-command": "^1.4.0",
    "@contentstack/cli-utilities": "^1.10.0",
    "@contentstack/types-generator": "^2.3.1",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@oclif/plugin-help": "^6.2.25",
    "@oclif/test": "^4.1.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^10.17.60",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^6.0.15",
    "eslint-config-oclif-typescript": "^3.1.13",
    "jest": "^26.6.3",
    "oclif": "^4.17.30",
    "ts-jest": "^26.5.6",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Contentstack-Solutions/contentstack-cli-tsgen",
  "keywords": [
    "contentstack",
    "cli",
    "plugin",
    "typescript"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "csdx",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Contentstack-Solutions/contentstack-cli-tsgen",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc --fix",
    "prepack": "rm -rf lib && tsc -b && oclif manifest && oclif readme",
    "test": "jest --testPathPattern=tests",
    "version": "oclif readme && git add README.md",
    "test:integration": "jest --testPathPattern=tests/integration"
  },
  "csdxConfig": {
    "shortCommandName": {
      "tsgen": "TSGEN"
    }
  }
}
